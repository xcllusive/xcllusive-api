{
  "containerDefinitions": [{
    "essential": true,
    "image": "VAR_IMAGE_NAME",
    "memoryReservation": 512,
    "memory": 512,
    "name": "VAR_APP_NAME",
    "portMappings": [{
      "containerPort": 80
    }],
    "environment": [{
        "name": "NODE_ENV",
        "value": "VAR_NODE_ENV"
      },
      {
        "name": "PORT",
        "value": "5000"
      },
      {
        "name": "JWT_EXPIRATION_HOURS",
        "value": "8"
      },
      {
        "name": "EMAIL_PORT",
        "value": "587"
      },
      {
        "name": "EMAIL_USER",
        "value": "businessinfo@xcllusive.com.au"
      },
      {
        "name": "AWS_REGION",
        "value": "ap-southeast-2"
      }
    ],
    "secrets": [{
      "name": "JWT_SECRET",
      "valueFrom": "arn:aws:ssm:ap-southeast-2:264038389685:parameter/app/mercury-nonprod/mercury-api-nonprod/JWT_SECRET"
    }, {
      "name": "MYSQL_URI_DEV",
      "valueFrom": "arn:aws:ssm:ap-southeast-2:264038389685:parameter/app/mercury-nonprod/mercury-api-nonprod/MYSQL_URI_DEV"
    }, {
      "name": "MYSQL_USERNAME_DEV",
      "valueFrom": "arn:aws:ssm:ap-southeast-2:264038389685:parameter/app/mercury-nonprod/mercury-api-nonprod/MYSQL_USERNAME_DEV"
    }, {
      "name": "MYSQL_PASSWORD_DEV",
      "valueFrom": "arn:aws:ssm:ap-southeast-2:264038389685:parameter/app/mercury-nonprod/mercury-api-nonprod/MYSQL_PASSWORD_DEV"
    }, {
      "name": "MYSQL_DATABASE_DEV",
      "valueFrom": "arn:aws:ssm:ap-southeast-2:264038389685:parameter/app/mercury-nonprod/mercury-api-nonprod/MYSQL_DATABASE_DEV"
    }]
  }],
  "family": "mercury-api-nonprod"
}
